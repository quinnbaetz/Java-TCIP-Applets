package edu.uiuc.power.displayobjects;

import java.awt.geom.Point2D;
import java.awt.geom.Point2D.Double;
import java.util.ArrayList;

import edu.uiuc.power.dataobjects.BranchData;

public class BranchCoordsAndData {
	ArrayList<Point2D.Double> coords;
	BranchData branch;
	public BranchCoordsAndData(ArrayList<Double> coords, BranchData branch) {
		super();
		this.coords = coords;
		this.branch = branch;
	}
	public ArrayList<Point2D.Double> getCoords() {
		return coords;
	}
	public BranchData getBranchData() {
		return branch;
	}
}
