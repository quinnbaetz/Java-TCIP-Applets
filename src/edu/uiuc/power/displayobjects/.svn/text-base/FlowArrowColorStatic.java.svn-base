package edu.uiuc.power.displayobjects;

import java.awt.Color;

import edu.uiuc.power.dataobjects.BranchData;

public class FlowArrowColorStatic implements FlowArrowColorProvider {

	Color staticColor;
	
	public FlowArrowColorStatic(Color staticColor) {
		this.staticColor = staticColor;
	}
	
	public Color getFlowArrowColor(BranchData branch) {
		return staticColor;
	}

}
