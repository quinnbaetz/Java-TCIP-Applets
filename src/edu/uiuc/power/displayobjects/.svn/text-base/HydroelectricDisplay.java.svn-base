package edu.uiuc.power.displayobjects;

import java.awt.geom.Point2D;
import java.io.IOException;

import edu.uiuc.power.dataobjects.GeneratorData;

public class HydroelectricDisplay extends GenericPlantDisplay {
	public HydroelectricDisplay(Point2D ULCorner, double deltaPerMouseClick,
			GeneratorData gendata) throws IOException {
		super(ULCorner, deltaPerMouseClick, gendata);
	}

	@Override
	public String getCaption() {
		return "Hydroelectric";
	}

	@Override
	public String getPathToImage() {
		return "/applet_images/hydroelectric.png";
	}
}
