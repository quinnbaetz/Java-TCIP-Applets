package edu.utoronto.power.lpsolver;
import java.io.Serializable;
/**
 * <p>An exception that can be thrown when doing things like
 * assigning variables to constraints that are out of bounds.</p>
 * <p>Copyright: Copyright (c) 2004</p>
 * @author Zeb Tate
 * @version 1.0
 */
public class LPException extends Exception implements Serializable {
  /**
   * LPException(String message) allows the exception to contain useful
   * information, i.e. the value that could not be set in a LPVariable.SetValue
   * call.
   *
   * @param message String
   */
  public LPException(String message) {
    super(message);
  }
}
