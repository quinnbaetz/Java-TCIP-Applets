package edu.uiuc.power.displayobjects;

import edu.uiuc.power.dataobjects.BranchData;

public class BranchThicknessStatic implements BranchThicknessProvider {
	
	double staticThickness;
	
	public BranchThicknessStatic(double staticThickness) {
		this.staticThickness = staticThickness;
	}

	public double getBranchThickness(BranchData branch) {
		return staticThickness;
	}

}
