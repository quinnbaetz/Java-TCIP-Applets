package edu.uiuc.power.dataobjects;

public class BranchOverloadMonitorParameters {
	
	boolean status;
	int numOfStepsBeforeOutage;
	public BranchOverloadMonitorParameters(boolean status, int numOfStepsBeforeOutage) {
		this.status = status;
		this.numOfStepsBeforeOutage = numOfStepsBeforeOutage;
	}

	public void setStatus(boolean status) {
		this.status = status;
	}
	
	public boolean getStatus() {
		return status;
	}
	
	public void setNumOfStepsBeforeOutage(int numOfStepsBeforeOutage) {
		this.numOfStepsBeforeOutage = numOfStepsBeforeOutage;
	}
	
	public int getNumOfStepsBeforeOutage() {
		return numOfStepsBeforeOutage;
	}
}
